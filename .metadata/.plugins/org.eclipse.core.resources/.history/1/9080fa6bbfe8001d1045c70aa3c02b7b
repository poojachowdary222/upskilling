package p1;

public class Encap {
private String name;
private int age;

public Encap(String string, int i) {
string =name;
i=age;

}


public String getName() {
	return name;
}
	public void setName(String name) {
		this.name= name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age= age;
	}
	}
