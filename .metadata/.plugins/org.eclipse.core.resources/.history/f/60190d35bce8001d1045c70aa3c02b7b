package p1;

public class Poly {

	void eat()
	{
		System.out.println("eating...");}  
	}  
	class Dog extends Poly{  
	void eat(){
		System.out.println("eating bread...");}  
	}  
	class Cat extends Poly{  
	void eat(){
		System.out.println("eating rat...");}  
	}  
	class Lion extends Poly{  
	void eat(){
		System.out.println("eating meat...");}  
}
